<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_navigation"
    app:startDestination="@id/first_fragment">
    <fragment
        android:id="@+id/first_fragment"
        android:name="vn.ntu.edu.TrungKhiem.frament.first_fragment"
        android:label="first_fragment"
        tools:layout="@layout/first_fragment" >
        <action
            android:id="@+id/action_first_fragment_to_second_fragmet"
            app:destination="@id/second_fragmet" />
    </fragment>
    <fragment
        android:id="@+id/second_fragmet"
        android:name="vn.ntu.edu.TrungKhiem.frament.second_fragment"
        android:label="second_fragmet"
        tools:layout="@layout/second_fragment" >
        <action
            android:id="@+id/action_second_fragmet_to_first_fragment"
            app:destination="@id/first_fragment" />
    </fragment>
</navigation>